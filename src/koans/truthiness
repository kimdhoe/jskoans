// 0을 제외한 모든 수는 참으로 칩니다.
const trueOrFalse = -7 ? true : false

assert.equal(trueOrFalse, _____)

// SEP

// 0은 거짓으로 칩니다.
const trueOrFalse = 0 ? true : false

assert.equal(trueOrFalse, _____)

// SEP

// 빈 문자열은 거짓으로 칩니다.
const trueOrFalse = '' ? true : false

assert.equal(trueOrFalse, _____)

// SEP

// 조건으로 쓰였을 때 null은 거짓으로 칩니다.
const trueOrFalse = null ? true : false

assert.equal(trueOrFalse, _____)

// SEP

// 조건으로 쓰였을 때 undefined는 거짓으로 칩니다.
const trueOrFalse = undefined ? true : false

assert.equal(trueOrFalse, _____)

// SEP

// 조건으로 쓰였을 때 NaN은 거짓으로 칩니다.
const trueOrFalse = NaN ? true : false

assert.equal(trueOrFalse, _____)